import File

main : String <IO>
main =
    let [mh] : ((Handle R) [0..1] <{Open, IOExcept}>) [2] = [openHandle ReadMode "LICENSE"] in
    let h       <- mh;
        h'      <- mh;
        [()]     <- closeHandle h';
        [(h, c)]  <- readChar h;
        [(h, c')] <- readChar h;
        [()]      <- closeHandle h
    in pure ((showChar c) `stringAppend` (showChar c'))
